{
    "name": "ezsystems/ezstudio-cron",
    "description": "Cron bundle",
    "type": "ezstudio-bundle",
    "authors": [
        {
            "name": "eZ Systems",
            "email": "dev-team@ez.no"
        }
    ],
    "license": "TTL-2.0",
    "autoload": {
        "psr-4": {
            "EzSystems\\Cron\\": "lib",
            "EzSystems\\Cron\\Tests\\": "tests/Cron",
            "EzSystems\\CronBundle\\": "bundle",
            "EzSystems\\CronBundle\\Tests\\": "tests/CronBundle"
        }
    },
    "require": {
        "cron/cron": "^1.0.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.7.1",
        "phpunit/phpunit": "~4.7.0"
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
